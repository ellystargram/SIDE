{
  "version": "SIDE: Mercury",
  "general": {
    "width": 1280,
    "height": 720,
    "isFullscreen": false,
    "defaultProjectName": "new Tera-forming"
  },
  "pallet": {
    "palletJsonAddress": "/module/pallet/pallet.json"
  },
  "handleBar": {
    "title": {
      "default": "SIDE: Mercury",
      "fontSize": 20,
      "font": "Arial"
    },
    "height": 50,
    "exitButton": {
      "text": "X",
      "fontSize": 20,
      "font": "Arial",
      "width": 50,
      "height": 50
    }
  },
  "commandBar": {
    "height": 130,
    "history": {
      "max": 10,
      "fontSize": 20,
      "font": "/resources/fonts/JetBrainsMono.ttf",
      "height": 100
    },
    "prompt": {
      "text": "",
      "fontSize": 20,
      "font": "/resources/fonts/JetBrainsMono.ttf",
      "height": 30
    },
    "executeButton": {
      "text": "Execute",
      "fontSize": 20,
      "font": "Arial",
      "width": 100,
      "height": 30
    }
  },
  "codeSpace": {
    "type": "python",
    "lineHeight": 15,
    "font": "/resources/fonts/JetBrainsMono.ttf",
    "fontSize": 20,
    "cursor": {
      "blinkRate": 500,
      "width": 2
    },
    "lineNumber": {
      "fontSize": 20,
      "font": "Arial"
    },
    "codeEditor": {
      "fontSize": 20,
      "font": "/resources/fonts/JetBrainsMono.ttf"
    },
    "mouseWheelSensitive": 10.0
  },
  "commands": {
    "system": {
      "jsonAddress": "/module/commandbar/system/system.json"
    }
  }
}