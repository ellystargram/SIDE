{
  "ideVersion": "Venus",
  "ideDefaultProjectName": "SIDE: Venus",
  "ideWindow": {
    "width": 800,
    "height": 600,
    "title": "SIDE: Venus",
    "fileBar": {
      "enabled": true,
      "border": {
        "enabled": true,
        "width": 1
      },
      "preferredWidth": 200,
      "preferredHeight": 200,
      "location": "West"
    },
    "terminalTabPane": {
      "enabled": true,
      "border": {
        "enabled": true,
        "width": 1
      },
      "preferredWidth": 200,
      "preferredHeight": 200,
      "location": "South",
      "controller": {
        "location": "North"
      },
      "tab":{
        "log": {
          "font": {
            "name": "JetBrains Mono",
            "size": 12
          }
        },
        "prompt": {
          "font": {
            "name": "JetBrains Mono",
            "size": 12
          }
        },
        "executeButton": {
          "font": {
            "name": "JetBrains Mono",
            "size": 12
          }
        }
      }
    },
    "editorPane": {
      "enabled": true,
      "location": "Center",
      "greetingMessage": "Simple IDE",
      "greetingTabTitle": "Welcome",
      "newFileTitle": "<new file>",
      "unsavedFileTitlePrefix": "* "
    }
  },
  "pallet": {
    "theme": "dark",
    "dark": {
      "name": "DarkMode",
      "path": "/resources/json/pallet/dark.json"
    },
    "light": {
      "name": "LightMode",
      "path": "/resources/json/pallet/light.json"
    },
    "custom": {
      "name": "CustomMode",
      "path": ""
    }
  }
}